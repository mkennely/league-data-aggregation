{
  "name": "data-aggregation",
  "version": "0.1.0",
  "description": "Fetch data from the Riot API and save to Mongo",
  "main": "dist",
  "author": "Mark Rinkel <rinkelm@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "babel src -d build && node ./build/main.js",
    "test": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "dependencies": {
    "kindred-api": "^2.0.83",
    "mongoose": "^4.11.10"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "eslint": "^4.6.1"
  }
}
